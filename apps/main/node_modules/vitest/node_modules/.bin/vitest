#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/tatiane.entler_tagin/Desktop/Tati/techchallenge-fiap-finance-app-2/techchallenge-fiap-financeapp/node_modules/.pnpm/vitest@4.0.17_@types+node@20.19.29_@vitest+browser-playwright@4.0.17_jiti@2.6.1_jsdom@27.4.0__6gmffnrhaqf6fnqfxubul4kxfe/node_modules/vitest/node_modules:/mnt/c/Users/tatiane.entler_tagin/Desktop/Tati/techchallenge-fiap-finance-app-2/techchallenge-fiap-financeapp/node_modules/.pnpm/vitest@4.0.17_@types+node@20.19.29_@vitest+browser-playwright@4.0.17_jiti@2.6.1_jsdom@27.4.0__6gmffnrhaqf6fnqfxubul4kxfe/node_modules:/mnt/c/Users/tatiane.entler_tagin/Desktop/Tati/techchallenge-fiap-finance-app-2/techchallenge-fiap-financeapp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/tatiane.entler_tagin/Desktop/Tati/techchallenge-fiap-finance-app-2/techchallenge-fiap-financeapp/node_modules/.pnpm/vitest@4.0.17_@types+node@20.19.29_@vitest+browser-playwright@4.0.17_jiti@2.6.1_jsdom@27.4.0__6gmffnrhaqf6fnqfxubul4kxfe/node_modules/vitest/node_modules:/mnt/c/Users/tatiane.entler_tagin/Desktop/Tati/techchallenge-fiap-finance-app-2/techchallenge-fiap-financeapp/node_modules/.pnpm/vitest@4.0.17_@types+node@20.19.29_@vitest+browser-playwright@4.0.17_jiti@2.6.1_jsdom@27.4.0__6gmffnrhaqf6fnqfxubul4kxfe/node_modules:/mnt/c/Users/tatiane.entler_tagin/Desktop/Tati/techchallenge-fiap-finance-app-2/techchallenge-fiap-financeapp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
